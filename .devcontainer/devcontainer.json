{
   "name": "SQLBox Dev Container",
   "dockerComposeFile": "docker-compose.yml",
   "service": "dev",
   "workspaceFolder": "/workspace/sqlbox",
   "customizations": {
      "vscode": {
         "settings": {
            "terminal.integrated.defaultProfile.linux": "bash"
         }
      }
   },
   "postCreateCommand": "pnpm install",
   "forwardPorts": [5173],
   "remoteUser": "node"
}
